<!DOCTYPE html>
<html>
<body>
    <h3>LISTADO DE GARANTIAS</h3>
    <table class="table">
        <tr>
            <th>Codigo</th>
            <th>Nombres de cliente</th>
            <th>Apellidos del cliente</th>
            <th>Tipo de Garantia</th>
            <th>Descripcion</th>
            <th>Valor de la garantia</th>
            <th>Estado</th>

        </tr>
        @foreach (var garantia in ViewBag.GetAllGarantias)
        {
    <tr>

        <th>@garantia.Id_garantia </th>
        <th>@garantia.nomCliente </th>
        <th>@garantia.apeCliente</th>
        <th>@garantia.Tip_garantia</th>
        <th>@garantia.Des_garantia</th>
        <th>@garantia.Valor_garantia (€) </th>
        <th>@garantia.Estado</th>
    </tr>
        }
    </table>
</body>
</html>


